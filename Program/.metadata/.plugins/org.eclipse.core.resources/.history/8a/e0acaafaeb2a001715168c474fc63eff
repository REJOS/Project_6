#include <cstdio>
#include "memorysystem.h"

int main() {

	printf("hi");

	return 0;
}




u_int_t ParsePageNum(u_int_t logical_addr) {
	return (logical_addr | PAGE_MASK) >> PAGE_SHIFT;
}

u_int_t ParseOffset(u_int_t logical_addr) {
	return (logical_addr | OFFSET_MASK);
}
